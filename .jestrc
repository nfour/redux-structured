{
  "verbose": true,
  "setupFiles": [ "<rootDir>/src/polyfill.ts" ],
  "transform": { ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js" },
  "testRegex": "\\.(test|spec)\\.tsx?$",
  "moduleFileExtensions": [ "ts", "tsx", "js" ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "./dist"
  ],
  "coverageDirectory": "./.coverage"

}
