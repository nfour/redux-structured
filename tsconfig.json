{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "jsx": "react",
    "moduleResolution": "node",
    "outDir":"./dist",
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "lib": [
      "es2015", "es2016"
    ]
  },
  "exclude": [ "dist" ],
  "include": [ "src/**/*" ]
}
